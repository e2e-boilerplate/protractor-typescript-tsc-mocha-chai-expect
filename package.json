{
  "author": "",
  "description": "Protractor end-to-end test automation boilerplate, using TypeScript, tsc, Mocha, and Chai",
  "devDependencies": {
    "@types/chai": "4.2.10",
    "@types/jasmine": "3.5.8",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "7.0.1",
    "@types/node": "12.12.29",
    "chai": "4.2.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "mocha": "7.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "protractor": "5.4.3",
    "rimraf": "3.0.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "chai",
    "expect",
    "mocha",
    "protractor",
    "tsc",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "name": "protractor-typescript-tsc-mocha-chai-expect",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplates/protractor-typescript-tsc-mocha-chai-expect.git"
  },
  "scripts": {
    "build": "npx tsc",
    "clean": "npx rimraf ./build/*",
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "pretty": "npx prettier --no-config './spec/*.ts' --write",
    "test": "npx protractor ./build/protractor.conf.js",
    "update-webdriver": "npx webdriver-manager update --standalone false --gecko",
    "update-webdriver:ci": "npx webdriver-manager update --versions.chrome=79.0.3945.36"
  },
  "version": "1.0.0"
}
